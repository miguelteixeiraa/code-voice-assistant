{
    "name": "code-voice-assistant",
    "version": "1.0.0",
    "description": "Coding together with a voice assistant!",
    "publisher": "code-insiders",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "clean": "rm -r out",
        "compile": "pnpm clean && tsc -p ./tsconfig.json",
        "start": "node out/src/extension.js",
        "watch": "tsc -watch -p ./",
        "test": "pnpm jest"
    },
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/vscode": "^1.72.0"
    },
    "devDependencies": {
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.4",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "eslint": "^8.25.0",
        "fastify": "^4.8.1",
        "jest": "^29.1.2",
        "prettier": "^2.7.1",
        "ts-jest": "^29.0.3",
        "typescript": "4.9.0-dev.20221014"
    }
}
